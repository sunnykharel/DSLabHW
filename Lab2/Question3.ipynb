   "source": [
    "\n",
    "import numpy as np\n",
    "\n",
    "mean = 0.0\n",
    "std = 1.0\n",
    "n = 150\n",
    "\n",
    "s = np.random.normal(mean, std, n)\n",
    "e = np.random.normal(mean, std, n)\n",
    "\n",
    "y = -3+0*s+e\n",
    "est_mean = np.sum(y)/n\n",
    "est_std = np.sqrt(np.sum((y-est_mean)**2)/n) #STD formula: sqrt(sum((x-mean)^2)/N)\n",
    "(est_mean, est_std)"
   ]
  }
